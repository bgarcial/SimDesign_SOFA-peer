# CMake package configuration file for the SerialCommunication library

@PACKAGE_INIT@

set(CMAKE_MODULE_PATH "${CMAKE_MODULE_PATH} @CMAKE_MODULE_PATH@")

find_package(SofaFramework REQUIRED)
find_package(SofaBase REQUIRED)
find_package(SofaGeneral REQUIRED)

if(NOT TARGET SerialComunication)
    include("${CMAKE_CURRENT_LIST_DIR}/SerialComunicationTargets.cmake")
endif()

set(SerialComunication_LIBRARIES SerialComunication)
set(SerialComunication_INCLUDE_DIRS @PACKAGE_SERIALCOMUNICATION_INCLUDE_DIR@)